<p id="notice"><%= notice %></p>

<table class="min-h-fit absolute overflow-scroll">
  <thead>
    <tr>
      <th>User</th>
      <th>username</th>
      <th>name</th>
      <th>Title</th>
      <th>Caption</th>
      <th>Price</th>
      <th>Photo</th>
      <th>On Market</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @images.each do |image| %>
      <tr>
        <td><%= image.user.username %></td>
        <td><%= image.user.name %></td>
        <td><%= image.user_id %></td>
        <td><%= image.title %></td>
        <td><%= image.caption %></td>
        <td><%= image.price %></td>
        <td><%= image_tag image.img.to_s %></td>
        <td><%= image.for_sale %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
