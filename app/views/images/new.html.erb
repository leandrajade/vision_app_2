<%= form_with model: [@user, @image],  local: true do |f| %>
    <h1>New Image</h1>


    <div class="field">
        <%= f.label :user_id %>
        <%= f.number_field :user_id %>
    </div>

    <div class="field">
        <%= f.label :title %>
        <%= f.text_field :title %>
    </div>

    <div class="field">
        <%= f.label :caption %>
        <%= f.text_field :caption %>
    </div>

    <div class="field">
        <%= f.label :price %>
        <%= f.number_field :price %>
    </div>

    <div class="field">
        <%= f.label :img %>
        <%= f.file_field :img %>
    </div>

    <div class="actions">
        <%= f.submit %>
    </div>

    <%= link_to 'Back', user_images_path %>
<% end %>