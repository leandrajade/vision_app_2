<p id="notice"><%= notice %></p>


<%= link_to 'My profile', user_path(@user)  %>
<%= button_to 'Sign out', destroy_user_session_path, method: :delete %>

<h1 class="text-3xl text-red-700 font-bold underline">Dashboards</h1>

<table class="text-2xl">
  <thead>
    <tr>
      <th>User</th>
      <th>username</th>
      <th>name</th>
      <th>Title</th>
      <th>Caption</th>
      <th>Price</th>
      <th>Photo</th>
      <th>On Market</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @images.each do |image| %>
      <tr>
        <td><%= image.user.username %></td>
        <td><%= image.user.name %></td>
        <td><%= image.user_id %></td>
        <td><%= image.title %></td>
        <td><%= image.caption %></td>
        <td><%= image.price %></td>
        <td><%= image_tag image.img.to_s %></td>
        <td><%= image.for_sale %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
